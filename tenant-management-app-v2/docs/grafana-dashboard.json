{
  "dashboard": {
    "id": null,
    "title": "Rentara Tenant Management - Application Monitoring",
    "tags": ["rentara", "tenant-management", "logging"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Error Rate Over Time",
        "type": "stat",
        "targets": [
          {
            "expr": "rate({app=\"rentara-tenant-management\", level=\"error\"}[5m])",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.01
                },
                {
                  "color": "red",
                  "value": 0.05
                }
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Total Errors (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "count_over_time({app=\"rentara-tenant-management\", level=\"error\"}[24h])",
            "legendFormat": "Total Errors",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "count(count by (user_id) ({app=\"rentara-tenant-management\"} | json | user_id != \"anonymous\"))",
            "legendFormat": "Active Users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Loki Transport Status",
        "type": "stat",
        "targets": [
          {
            "expr": "count_over_time({app=\"rentara-tenant-management\", log_type=\"loki_config\"}[1h])",
            "legendFormat": "Loki Events",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Error Types Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "count by (log_type) ({app=\"rentara-tenant-management\", level=\"error\"} | json)",
            "legendFormat": "{{log_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "User Actions Timeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "count_over_time({app=\"rentara-tenant-management\", log_type=\"user_action\"}[5m])",
            "legendFormat": "User Actions/5min",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 16,
          "x": 8,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "API Response Times",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg_over_time({app=\"rentara-tenant-management\", log_type=\"api_request\"} | json | unwrap duration[5m])",
            "legendFormat": "Avg Response Time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ms"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Recent Error Details",
        "type": "logs",
        "targets": [
          {
            "expr": "{app=\"rentara-tenant-management\", level=\"error\"} | json",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": true,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "Authentication Events",
        "type": "timeseries",
        "targets": [
          {
            "expr": "count_over_time({app=\"rentara-tenant-management\", log_type=\"auth_state_change\"}[10m])",
            "legendFormat": "Auth Events/10min",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "Database Operations",
        "type": "timeseries",
        "targets": [
          {
            "expr": "count_over_time({app=\"rentara-tenant-management\", log_type=\"supabase_error\"}[5m])",
            "legendFormat": "DB Errors/5min",
            "refId": "A"
          },
          {
            "expr": "count_over_time({app=\"rentara-tenant-management\", log_type=\"db_query_success\"}[5m])",
            "legendFormat": "DB Success/5min",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 24
        }
      },
      {
        "id": 11,
        "title": "Session Activity Heatmap",
        "type": "heatmap",
        "targets": [
          {
            "expr": "count_over_time({app=\"rentara-tenant-management\"} | json[1h]) by (session_id)",
            "legendFormat": "{{session_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "spectrum"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 24
        }
      },
      {
        "id": 12,
        "title": "Real-time Log Stream",
        "type": "logs",
        "targets": [
          {
            "expr": "{app=\"rentara-tenant-management\"} | json",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 32
        }
      }
