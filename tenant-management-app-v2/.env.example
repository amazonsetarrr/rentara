VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Loki/Grafana Logging Configuration
# Enable/disable Loki logging (true/false)
VITE_LOKI_ENABLED=false

# Loki Push API endpoint (e.g., https://your-loki-instance.com)
VITE_LOKI_ENDPOINT=

# Loki authentication (if required)
VITE_LOKI_USERNAME=
VITE_LOKI_PASSWORD=

# Loki tenant/organization ID (for multi-tenant setups)
VITE_LOKI_TENANT=rentara

# Example production configuration:
# VITE_LOKI_ENABLED=true
# VITE_LOKI_ENDPOINT=https://logs-prod-us-central1.grafana.net
# VITE_LOKI_USERNAME=your_grafana_user_id
# VITE_LOKI_PASSWORD=your_grafana_api_key
# VITE_LOKI_TENANT=your_tenant_id